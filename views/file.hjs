<div id="receiver-container">
    <h1>You've get a file.</h1>
    {{#file}}
        <div class="file-info">
            <div class="file-name">{{ name }}</div>
            <div class="file-size">{{ size }}</div>
        </div>
    {{/file}}
    <a class="receive-button" href="/get/{{transferId}}">Receive file</a>

</div>
